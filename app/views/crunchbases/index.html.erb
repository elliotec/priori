<h1>Listing crunchbases</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @crunchbases.each do |crunchbasis| %>
      <tr>
        <td><%= link_to 'Show', crunchbasis %></td>
        <td><%= link_to 'Edit', edit_crunchbasis_path(crunchbasis) %></td>
        <td><%= link_to 'Destroy', crunchbasis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Crunchbase', new_crunchbasis_path %>
